{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES2020",
    "outDir": "./dist",
    "rootDir": "./scripts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "types": ["fvtt-types", "jest"],
  },
  "include": [
    "scripts/**/*.ts"
  ]
}
